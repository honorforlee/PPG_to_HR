% Ivan NY HANITRA - Master thesis
%       -- Select frame of timeline and event  --

function [kx_frame,tx_frame,sx_frame,sx_N_frame, dhi_frame,dlo_frame,note_x_frame] = frame_select(kx,tx,sx,sx_N, dhi,dlo,note_x, frame_init,frame_end)
index_x = find(tx >= frame_init & tx <= frame_end);
kx_frame = kx(index_x);
tx_frame = tx(index_x);
sx_frame = sx(index_x);
sx_N_frame = sx_N(index_x);

dhi_frame = dhi(index_x);
dlo_frame = dlo(index_x);
note_x_frame = note_x(index_x);

